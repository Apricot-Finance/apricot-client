{
  "name": "@mercurial-finance/apricot-sdk",
  "version": "0.17.5",
  "description": "TS SDK for Apricot on Solana",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "typings": "dist/index.d.ts",
  "keywords": [
    "Apricot",
    "Solana"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsup",
    "print": "node dist/tools/doPrint.js",
    "trans": "node dist/tools/doTrans.js",
    "user-stats": "node dist/tools/doParse.js public user-stats",
    "user": "node dist/tools/doParse.js public user",
    "pool": "node dist/tools/doParse.js public pool",
    "loadjs": "node dist/index.js",
    "lint": "eslint src",
    "fix": "eslint --fix src"
  },
  "dependencies": {
    "@apricot-lend/solana-swaps-js": "0.1.18",
    "@project-serum/borsh": "^0.2.3",
    "@pythnetwork/client": "^2.5.1",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.37.0",
    "decimal.js": "^10.3.1",
    "tiny-invariant": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "tsup": "^6.5.0",
    "typescript": "^4.4.3"
  }
}
